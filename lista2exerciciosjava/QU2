import java.util.Scanner;

    public class Q2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("Informe N: ");
        int n = sc.nextInt();
        int[] quadrados = new int[n];

        for (int i = 0; i < n; i++) {
            quadrados[i] = (i + 1) * (i + 1);
        }

        System.out.println("Quadrados de 1 atÃ© " + n + ":");
        for (int q : quadrados) {
            System.out.print(q + " ");
        }
        sc.close();
    }
}

